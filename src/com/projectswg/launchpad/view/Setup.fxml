         <children>
            <Pane fx:id="setupDisplayPane" maxWidth="Infinity" prefHeight="50.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Button fx:id="pswgFolderButton" maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" text="Select ProjectSWG Folder" VBox.vgrow="ALWAYS" />
